{
    "headers": [
        {
            "source": "/germanier-paris-2026",
            "headers": [
                {
                    "key": "X-Robots-Tag",
                    "value": "index, follow, max-image-preview:large"
                }
            ]
        },
        {
            "source": "/og-germanier-2026.jpg",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}