{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Content-Security-Policy",
                    "value": "frame-ancestors 'self' https://shipbyx.com https://www.shipbyx.com https://ivyjstudio.shipbyx.com;"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "ALLOW-FROM https://shipbyx.com"
                }
            ]
        },
        {
            "source": "/germanier-paris-2026",
            "headers": [
                {
                    "key": "X-Robots-Tag",
                    "value": "index, follow, max-image-preview:large"
                }
            ]
        },
        {
            "source": "/germanier-paris-2026-ivy-j-studio-headpiece-lisa-rinna.jpg",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "rewrites": []
}